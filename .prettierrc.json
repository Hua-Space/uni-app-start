{
  "tabWidth": 2,
  "printWidth": 120,
  "proseWrap": "never",
  "eslintIntegration": true,
  "stylelintIntegration": true,
  "bracketSameLine": true,
  "singleQuote": true,
  "semi": false,
  "bracketSpacing": true,
  "trailingComma": "none",
  "endOfLine": "lf",
  "ignorePath": ".prettierignore",
  "vueIndentScriptAndStyle": false,
  "htmlWhitespaceSensitivity": "ignore"
}
